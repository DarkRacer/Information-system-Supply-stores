<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="145.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Product.AdmissionProduct">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="31.0" prefWidth="162.0" text="Выберите товар*">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="Product" layoutX="185.0" layoutY="14.0" prefHeight="31.0" prefWidth="174.0" />
      <Label layoutX="14.0" layoutY="45.0" prefHeight="31.0" prefWidth="231.0" text="Количество поступившего товара">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="colTF" layoutX="245.0" layoutY="48.0" prefHeight="26.0" prefWidth="112.0" />
      <Button fx:id="saveProduct" layoutX="14.0" layoutY="87.0" mnemonicParsing="false" onAction="#save" prefHeight="31.0" prefWidth="154.0" text="Сохранить">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="204.0" layoutY="87.0" mnemonicParsing="false" onAction="#cancel" prefHeight="31.0" prefWidth="154.0" text="Отмена">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="127.0" prefHeight="18.0" prefWidth="358.0" text="*Если поступил новый товар, то необходимо его сначала добавить в базу">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
